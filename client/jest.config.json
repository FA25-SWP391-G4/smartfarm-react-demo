{
  "transform": {
    "^.+\\.[t|j]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(axios|react-router|react-router-dom)/).*/"
  ],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/src/setupTests.js"
  ],
  "testEnvironment": "jsdom"
}
